<%- include("header"); -%>

    <div class="box" id="heading">
        <h1> <%= listTittle %>!</h1>
    </div>

    <div class="box">
        <% items.forEach( (item) => { %>
        <div class="item">
            <input type="checkbox">
                <p> <%= item %></p>
        </div>
        <%})%>
        <form  class="item" action="/" method="post" >
            <input type="text" name="listItems" placeholder="New Item" autocomplete="off">
            <button type="submit"name="list" value=<%=listTittle%>>+</button>
        </form>
    </div>
<%- include("footer"); -%>